{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "env": {
    "es6": true
  },
  "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        },
        "sourceType": "module"
  },
  "rules": {
    "strict": 0,
    "func-names": 0,
    "space-before-function-paren": 0,
    "no-unused-vars": 1,
    "no-param-reassign": 0,
    "max-len": 0,
    "arrow-body-style": 0,
    "prefer-arrow-callback": 0,
    "new-cap": 0,
    "class-methods-use-this": 0
  },
  "globals"   : {
    "describe"   : false,
    "it"         : false,
    "before"     : false,
    "beforeEach" : false,
    "after"      : false,
    "afterEach"  : false
  }
}
